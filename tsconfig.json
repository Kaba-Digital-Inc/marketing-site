{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "src/**/*",
    "node_modules/astro/tsconfigs/${configDir}/.astro/types.d.ts",
    "node_modules/astro/tsconfigs/${configDir}/**/*"
  ],
  "exclude": [
    "node_modules",
    "node_modules/astro/tsconfigs/${configDir}/dist"
  ]
}
